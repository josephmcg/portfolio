{
  "extends": "@josephmcg/tsconfig/next.json",
  "compilerOptions": {
    "paths": {
      "~/*": ["./src/*"]
    },
    "verbatimModuleSyntax": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "reset.d.ts"
  ],
  "exclude": ["node_modules"]
}
